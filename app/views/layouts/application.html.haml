<!DOCTYPE html>
%html
	%head
		%title 
			= @title 
		= javascript_include_tag :defaults 
		= stylesheet_link_tag 'screen', :media => 'screen' 
		= csrf_meta_tag
	%body
		.main_container
			.header	
				= image_tag 'logo.png', :class => 'round'
			.navigation{:class => 'round'}
				%ul
					%li= link_to "Home", root_path
					%li= link_to "Help", help_path
					%li= link_to "Sign-In", #
			.clear
			.content{:class => 'round'}
				= yield 
			= render :partial => 'shared/footer'
